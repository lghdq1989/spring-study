cat << end
  01.发送security,mange,pub
  02.发送job,product
end
read num
case $num in
   '01')
      echo 01
      ;;
   '02')
     echo 02
     ;;
  *)
     exit 1
esac
exit 0



select vegetable in "beans" "carrots" "potatoes" "onions" "rutabagas"
do
  echo
  echo "Your favorite veggie is $vegetable."
  echo "Yuck!"
  echo
  break  # 如果这里没有 'break' 将不停循环进行选择
done

exit 0